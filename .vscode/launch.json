{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby Develop",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": ["develop"],
      "runtimeArgs": ["--inspect"],
      "console": "integratedTerminal",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v18.20.7/bin/node",
      "preLaunchTask": "gatsby-clean"
    },
    {
      "name": "Gatsby Build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": ["build"],
      "runtimeArgs": ["--inspect"],
      "console": "integratedTerminal",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v18.20.7/bin/node",
      "preLaunchTask": "gatsby-clean"
    }
  ]
}
